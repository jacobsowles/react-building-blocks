{
  "name": "react-building-blocks",
  "description": "A collection of React components used by virtually every web application",
  "author": {
    "name": "Jacob Sowles",
    "email": "jrsowles@gmail.com"
  },
  "version": "0.1.1",
  "scripts": {
    "dev": "cd docs && npm install && open 'http://localhost:8080' && NODE_ENV=development webpack-dev-server --inline --hot",
    "docs:release": "cd docs && rm -rf node_modules && npm install && NODE_ENV=production webpack --progress && git add -A && git commit -m \"Docs Version: $(node -p 'require(\"../package.json\").version')\"",
    "release": "eslint src && babel src --out-dir dist",
    "test": "BABEL_JEST_STAGE=0 jest || true"
  },
  "peerDependencies": {
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.1",
    "eslint-config-defaults": "^9.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-react": "^6.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "jest": "^16.0.1",
    "jest-cli": "^16.0.1",
    "jsx-loader": "^0.13.2",
    "live-server": "^0.8.1",
    "node-sass": "^4.1.0",
    "react-addons-test-utils": "^15.4.1",
    "react-router": "^3.0.0",
    "react-test-renderer": "^15.3.2",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "webpack-livereload-plugin": "^0.8.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jrsowles/react-building-blocks.git"
  },
  "homepage": "https://github.com/jrsowles/react-building-blocks",
  "bugs": {
    "url": "https://github.com/jrsowles/react-building-blocks/issues"
  },
  "main": "dist/index.js",
  "files": [
    "*.md",
    "dist",
    "docs",
    "LICENSE",
    "src"
  ],
  "keywords": [
    "react-building-blocks",
    "react",
    "react-component",
    "component",
    "UI",
    "library"
  ],
  "license": "MIT",
  "jest": {
    "testRegex": "(/tests/.*|\\.test)\\.(js|jsx)$",
    "scriptPreprocessor": "./node_modules/babel-jest",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "es6"
    ],
    "modulePathIgnorePatterns": [
      ".*dist.*"
    ]
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ]
  }
}
