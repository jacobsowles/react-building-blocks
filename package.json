{
  "name": "react-building-blocks",
  "description": "A collection of React components used by virtually every web application",
  "author": {
    "name": "Jacob Sowles",
    "email": "jrsowles@gmail.com"
  },
  "version": "0.1.0",
  "scripts": {
    "build-dev": "npm run test && webpack --define process.env.NODE_ENV='\"development\"' --progress",
    "build-example": "npm run test && webpack --define process.env.NODE_ENV='\"development\"' --progress ./example/index.js ./example/bundle.js",
    "build-prod": "npm run test && webpack -p --define process.env.NODE_ENV='\"production\"' --progress",
    "start": "python -m SimpleHTTPServer 8000 example",
    "test": "BABEL_JEST_STAGE=0 jest || true",
    "watch": "webpack --define process.env.NODE_ENV='\"development\"' --progress --watch",
    "watch-example": "webpack --define process.env.NODE_ENV='\"development\"' --progress --watch ./example/index.js ./example/bundle.js"
  },
  "peerDependencies": {
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-root-import": "^4.1.3",
    "babel-runtime": "^6.11.6",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.1",
    "eslint-config-defaults": "^9.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-react": "^6.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "jest": "^16.0.1",
    "jest-cli": "^16.0.1",
    "jsx-loader": "^0.13.2",
    "node-sass": "^4.1.0",
    "react-addons-test-utils": "^15.4.1",
    "react-test-renderer": "^15.3.2",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-livereload-plugin": "^0.8.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jrsowles/react-building-blocks.git"
  },
  "homepage": "https://github.com/jrsowles/react-building-blocks",
  "bugs": {
    "url": "https://github.com/jrsowles/react-building-blocks/issues"
  },
  "files": [
    "*.md",
    "dist",
    "LICENSE",
    "src"
  ],
  "keywords": [
    "react-building-blocks",
    "react",
    "react-component",
    "component"
  ],
  "license": "MIT",
  "main": "dist/bundle.js",
  "jest": {
    "testRegex": "(/tests/.*|\\.test)\\.(js|jsx)$",
    "scriptPreprocessor": "./node_modules/babel-jest",
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ]
  }
}
